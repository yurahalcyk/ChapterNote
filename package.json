{
  "name": "chapternote",
  "private": "true",
  "version": "1.0.0",
  "description": "A book note taking app.",
  "main": "index.js",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "dev:frontend": "npm run dev --workspace=@chapternote/frontend",
    "dev:backend": "npm run dev --workspace=@chapternote/backend",
    "test:backend": "npm run test --workspace=@chapternote/backend",
    "test:frontend": "npm run test --workspace=@chapternote/frontend",
    "build": "tsc -b",
    "clean": "tsc -b --clean",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@swc/core": "^1.15.5",
    "@swc/jest": "^0.2.39",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.10.1",
    "@typescript-eslint/eslint-plugin": "^8.47.0",
    "@typescript-eslint/parser": "^8.47.0",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "globals": "^16.5.0",
    "jest": "^30.2.0",
    "prettier": "^3.6.2",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.47.0"
  },
  "dependencies": {
    "dotenv": "^17.2.3"
  }
}
